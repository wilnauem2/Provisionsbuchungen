    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.56.0",
